<network_configuration>
<device_information>
<device>
<name>uc berkeley 4.3bsd</name>
<personality>4.3BSD</personality>
<action tcp="open" udp="closed" icmp="open"/>
<service_list>
<service protocol="tcp" port="22" execute="closed"/>
<service protocol="tcp" port="1024" execute="closed"/>
<service protocol="tcp" port="3" execute="filtered"/>
<service protocol="tcp" port="5" execute="filtered"/>
<service protocol="udp" port="105" execute="open"/>
<service protocol="udp" port="205" execute="open"/>
<service protocol="udp" port="505" execute="filtered"/>
<service protocol="icmp" port="0" execute="python2.7 honeyd/scripts/test.py"/>
</service_list>
<bind_list>
<bind ip="10.0.2.100"/>
<bind ip="10.0.2.101"/>
</bind_list>
</device>
<device>
<name>cisco router</name>
<personality>Cisco 1700 router</personality>
<action tcp="open" udp="closed" icmp="open"/>
<service_list>
</service_list>
<bind_list>
<bind ip="10.0.2.200"/>
<bind ip="10.0.2.201"/>
<bind ip="10.0.2.202"/>
<bind ip="10.0.2.203"/>
</bind_list>  
</device>
</device_information>
<routing_information>
<router ip="10.0.2.200" subnet="10.0.0.0/16" entry="true">
<connect>10.0.2.201</connect>
<link>10.0.0.0/24</link>
</router>
<router ip="10.0.2.201" subnet="10.0.0.0/16" entry="false">
<link>10.0.0.0/24</link>
</router>
</routing_information>
<external></external>
</network_configuration>
